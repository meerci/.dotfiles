[user]
	email = chenbao.cs@qq.com
	name = chenbao
[alias]
	a = add
	s = status
	st = status
	d = diff
	df = diff
	dd = diff --staged
	co = checkout
	br = branch
	bra = branch --all
	cm = commit
	cmm = commit -m
	cms = commit --squash
	cmae = commit --amend
	cma = commit --amend --no-edit
	cmf = commit --fixup
	ll = log --graph
	l = log --oneline --graph
	logl = log --oneline --graph
	rst = reset
	rss = restore --staged
	rs = restore
	cp = cherry-pick
  cpa = cherry-pick --abort
  cpc = cherry-pick --continue
	patch = cherry-pick --no-commit
	pt = cherry-pick --no-commit
	rb = rebase --autosquash -i
  rba = rebase --abort
  rbc = rebase --continue
  mga = merge --abort
  mgc = merge --continue
[core]
	editor = nvim
[pull]
	rebase = true
[push]
	default = simple
[merge]
	tool = diffview
[mergetool]
	prompt = false
	keepBackup = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
